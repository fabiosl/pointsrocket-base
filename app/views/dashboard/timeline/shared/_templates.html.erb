<script id="template-user-who-share" type="x-tmpl-mustache">
  <div class="media">
    <span class="pull-left">
      <a href="<%= users_path %>/{{ user.id }}" class="text-default">
        <img src="{{ user.avatar }}" class="media-object img-circle" alt="" width="50px" height="50px;">
      </a>
    </span>
    <span class="media-body">
      <span class="media-heading">
        <a href="<%= users_path %>/{{ user.id }}" class="text-default">
          {{ user.name }}
        </a>
      </span>
      <span class="media-text ellipsis nm">{{ formatted_created_at }}</span>
    </span>
  </div>
</script>

<script id="template-share-create" type="x-tmpl-mustache">
  <%= render partial: "dashboard/timeline/advocacy_share_create" %>
</script>

<script id="template-share-success" type="x-tmpl-mustache">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header text-center">
        <button type="button" class="close" data-dismiss="modal">Ã—</button>
        <div class="ico-{{ share_ok.social_slug }} mb15 mt15" style="font-size:36px; color: {{ share_ok.color }};"></div>
        <h3 class="semibold modal-title text-primary"><%= t('views.employee_advocacy.post_shared') %></h3>
      </div>
      <div class="modal-body">
        <p><%= t('views.employee_advocacy.success_share') %>.</p>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">
          <%= t('views.employee_advocacy.close') %>
        </button>
      </div>
    </div>
  </div>
</script>

