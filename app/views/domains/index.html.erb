<div class="row">
  <div class="col-md-8 col-md-offset-2">
    <h4><%= t('views.domains.index.title') %></h4>
    <div class="row">
    <% @domains.each do |domain| %>
      <div class="col-md-4">
        <%= render partial: current_user.domain_access_partial(domain),
                   locals: { domain: domain }
        %>
      </div>
    <% end %>
    </div>
  </div>
</div>
